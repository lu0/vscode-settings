{
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.defaultFormatter": "ms-python.python",
    },
    "python.linting.enabled": false,
    "python.linting.lintOnSave": false,
    "python.linting.pylintEnabled": false,
    "python.linting.pydocstyleEnabled": false,
    "python.linting.pycodestyleEnabled": false,
    "python.languageServer": "Pylance",
    "python.pythonPath": "/usr/bin/python",
    "python.terminal.activateEnvironment": false,
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[sql]": {
        "editor.defaultFormatter": "mtxr.sqltools"
    },
    "terminal.integrated.tabs.enabled": false,
    "terminal.integrated.experimentalRestore": true,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.copyOnSelection": true,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.cursorMoveOnType": false,
    "editor.autoClosingQuotes": "never",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "Source Code Pro Medium",
    "terminal.integrated.fontWeightBold": "normal",
    "editor.fontSize": 16,
    "editor.minimap.maxColumn": 150,
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "editor.lineHeight": 26,
    "debug.console.fontSize": 16,
    "debug.console.historySuggestions": false,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.letterSpacing": 1,
    "window.menuBarVisibility": "hidden",
    "window.enableMenuBarMnemonics": false,
    // "window.titleBarStyle": "native",
    "window.titleBarStyle": "custom",
    "window.title": "${rootName}",
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "editor.wordWrap": "on",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.editor.closeEmptyGroups": true,
    "zenMode.hideTabs": false,
    "keyboard.dispatch": "keyCode",
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "scm.diffDecorations": "none",
    "customizeUI.fontSizeMap": {
        "13px": "14px",
        "12px": "13px",
        "tab-title": "14px", // Used for editor tab titles
        "monospace": "18px", // Used for monospace fonts in user interface
    },
    "customizeUI.font.regular": "WeblySleek UI Normal",
    "customizeUI.listRowHeight": 22,
    "customizeUI.activityBarHideSettings": true,
    "customizeUI.activityBar": "regular",
    "browser-preview.chromeExecutable": "/usr/bin/vivaldi",
    "browser-preview.format": "png",
    "browser-preview.startUrl": "github.com/lu0",
    "markdown.preview.fontSize": 18,
    "workbench.statusBar.visible": true,
    "files.associations": {
        "bashrc": "shellscript",
        "*.html": "html"
    },
    "editor.letterSpacing": 0.1,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modifications",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "terminal.integrated.allowMnemonics": true,
    "workbench.iconTheme": "material-icon-theme",
    "sqltools.connectionExplorer.groupConnected": true,
    "sqltools.historySize": 1000,
    "sqltools.highlightQuery": true,
    "sqltools.format": {
        "reservedWordCase": "upper",
        "linesBetweenQueries": 2,
        "language": "sql",
    },
    "sqltools.results": {
        "customization": {
            "font-family": "Source Code Pro",
            "font-size": "10"
        }
    },
    "jupyter.alwaysTrustNotebooks": true,
    "workbench.editor.tabCloseButton": "off",
    "explorer.autoReveal": "focusNoScroll",
    "explorer.decorations.badges": true,
    "explorer.decorations.colors": true,
    "problems.decorations.enabled": true,
    "problems.autoReveal": true,
    "problems.showCurrentInStatus": true,
    "outline.problems.badges": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "markdown.extension.tableFormatter.normalizeIndentation": true,
    "workbench.editorAssociations": {
        "*.md": "default",
        "*.svg": "default",
        "*.ipynb": "jupyter-notebook"
    },
    "java.configuration.runtimes": [{
            "name": "JavaSE-15",
            "path": "/usr/lib/jvm/jdk-15.0.1+9",
            "default": false
        },
        {
            "name": "JavaSE-1.8",
            "path": "/usr/lib/jvm/jdk1.8.0_191",
            "default": true
        }
    ],
    "java.home": "/usr/lib/jvm/jdk-15.0.1+9",
    "gitlens.hovers.currentLine.over": "line",
    "eslint.lintTask.enable": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.statusBar.enabled": true,
    "gitlens.currentLine.format": "${id} ${date} ${authorNotYou} ${message}",
    "gitlens.hovers.enabled": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "extensions.ignoreRecommendations": true,
    "restoreTerminals.runOnStartup": false,
    "restoreTerminals.terminals": [
        {
            "splitTerminals": [
                {
                    "name": "files",
                    "commands": ["ll"]
                },
                {
                    "name": "git",
                    "commands": ["git log -5 --format=fuller"]
                }
            ]
        },
        {
            "splitTerminals": [
                {
                    "name": "Python Shell",
                    "commands": ["python3"]
                },
                {
                    "name": "JS Shell",
                    "commands": ["js"]
                }
            ]
        },
    ],
    "workbench.editor.decorations.badges": true,
    "search.experimental.searchInOpenEditors": true,
    "angular.enable-experimental-ivy-prompt": false,
    "java.autobuild.enabled": false,
    "markdown-pdf.type": [
        "pdf"
    ],
    "markdown-pdf.displayHeaderFooter": false,
    "markdown-pdf.emoji": false,
    "markdown-pdf.format": "Letter",
    "markdown-pdf.margin.left": "1.5cm",
    "markdown-pdf.margin.bottom": "1.5cm",
    "markdown-pdf.margin.right": "1.5cm",
    "markdown-pdf.margin.top": "1.5cm",
    "markdown-pdf.footerTemplate": "<div style=\"font-size: 9px; margin: 0 auto;\"> <span class='pageNumber'></span> / <span class='totalPages'></span></",
    "markdown-pdf.breaks": true,
    "java.project.importOnFirstTimeStartup": "automatic",
    "git.ignoreLegacyWarning": true,
    "editor.autoIndent": "brackets",
    "editor.detectIndentation": false,
    "docker.containers.label": "ContainerName",
    "remote.SSH.connectTimeout": 60,
    "workbench.colorTheme": "Interplanetary Craft Dark",
    "workbench.preferredDarkColorTheme": "Interplanetary Craft Dark",
    "workbench.preferredLightColorTheme": "Silver Gray",
    "gitlens.views.fileHistory.files.compact": false,
    "workbench.activityBar.visible": false,
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "security.workspace.trust.enabled": false,
    "terminal.integrated.defaultProfile.linux": "bash",
    "editor.definitionLinkOpensInPeek": false,
    "workbench.startupEditor": "none",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "vim.ignorecase": true,
    "vim.startofline": true,
    "vim.showmodename": true,
    "vim.useCtrlKeys": false,
    "vim.smartRelativeLine": true,
    "vim.startInInsertMode": false,
    "vim.useSystemClipboard": true,
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "line",
    "vim.cursorStylePerMode.visual": "underline",
    "vim.cursorStylePerMode.visualblock": "underline",
    "vim.cursorStylePerMode.visualline": "underline",
    "vim.statusBarColorControl": true,
    "vim.statusBarColors.normal": "#FF4B33",
    "vim.statusBarColors.insert": "#212121",
    "vim.statusBarColors.visual": "#FF59CD",
    "vim.statusBarColors.searchinprogressmode": "#8C6BC8",
    "vim.mouseSelectionGoesIntoVisualMode": false,
}