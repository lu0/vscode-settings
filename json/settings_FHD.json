{
    "terminal.integrated.experimentalRestore": true,
    "editor.cursorStyle": "block",
    "editor.fontFamily": "'Source Code Pro', 'monospace', monospace, 'Segoe UI'",
    "editor.fontSize": 15,
    "debug.console.fontSize": 15,
    "terminal.integrated.fontSize": 15,
    "editor.minimap.maxColumn": 150,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "workbench.colorTheme": "Monokai",
    "workbench.editor.showTabs": true,
    "workbench.activityBar.visible": false,
    "window.zoomLevel": 0,
    "window.menuBarVisibility": "toggle",
    "breadcrumbs.enabled": true,
    "editor.renderWhitespace": "all",
    "editor.renderControlCharacters": false,
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "line",
    "vim.cursorStylePerMode.visual": "underline",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m -javaagent:\"~/.vscode/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
    "editor.wordWrap": "on",
    "workbench.editor.closeEmptyGroups": true,
    "zenMode.hideTabs": false,
    "keyboard.dispatch": "keyCode",
    "workbench.editor.enablePreviewFromQuickOpen": false
